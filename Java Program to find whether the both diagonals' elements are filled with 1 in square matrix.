import java.util.*;
class c{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        boolean flag=true;
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                int a=s.nextInt();
                if(j==i&&a!=1){
                    flag=false;
                    break;
                }
            }
        }
        if(flag)
        System.out.println("Both the diagonals are filled with 1");
        else
        System.out.println("A diagonal contains something other than 1");
    }
}
