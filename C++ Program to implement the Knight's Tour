#include <iostream>
using namespace std;
int N,board[100][100];
const int moves[8][2] = 
{
    {2, 1}, {1, 2},
    {-1, 2}, {-2, 1},
    {-2, -1}, {-1, -2},
    {1, -2}, {2, -1}
};
bool isSafe(int x, int y) 
{
    return (x>=0&&y>=0&&x<N&&y<N&&board[x][y]==-1);
}
void printSolution() 
{
    for (int i = 0; i < N; ++i) 
    {
        for (int j = 0; j < N; ++j) 
        {
            std::cout << board[i][j] << " ";
        }
        std::cout << std::endl;
    }
}
bool solveKnightTour(int x, int y, int movei) {
   if(movei==N*N)
   return true;
   else{
       int nx,ny;
       for(int i = 0;i<8;i++){
           nx=x+moves[i][0];
           ny=y+moves[i][1];
           if(isSafe(nx,ny)){
               board[nx][ny]=movei;
               if(solveKnightTour(nx,ny,movei+1)){
                   return true;
               }
               else{
                   board[nx][ny]=-1;
               }
           }
       }
   }
   return false;
}
int main() {
    cin>>N;
    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < N; ++j) {
            board[i][j] = -1;
        }
    }
    int start_x = 0;
    int start_y = 0;
    board[start_x][start_y] = 0;
    if (solveKnightTour(start_x, start_y, 1)) {
        std::cout << "Solution exists: \n";
        printSolution();
    } else {
        std::cout << "No solution exists.\n";
    }
    return 0;
}
